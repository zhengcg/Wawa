<view class='listBox'>
  <view><text>名称</text>{{info.title}}</view>
  <view><text>厂家</text>{{info.vender}}</view>
  <view><text>剂型</text>{{info.dosage}}</view>
  <view><text>规格</text>{{info.spec}}</view>
  <view>
    <text>单价：</text>
    {{info.price}}
  </view>
  <view>
    <text>数量：</text>
    {{info.amount}}
  </view>
</view>
<view style='height:222rpx;'></view>
<view class='footer'>
  <button bindtap='submit'>返回</button>
</view>
